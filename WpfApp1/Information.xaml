        <Button Content="Supplies" HorizontalAlignment="Left" Height="30" Margin="89,365,0,0" VerticalAlignment="Top" Width="69" Click="Button_Click"/>
        <Label Content="Indetificator" HorizontalAlignment="Left" Height="23" Margin="5,324,0,0" VerticalAlignment="Top" Width="84"/>
        <Button Content="DemandsHouse" HorizontalAlignment="Left" Height="30" Margin="322,347,0,0" VerticalAlignment="Top" Width="154" Click="Button_Click2" />
        <Button Content="DemandsApartaments" HorizontalAlignment="Left" Height="30" Margin="322,377,0,0" VerticalAlignment="Top" Width="154" Click="Button_Click3" />

    </Grid>
</Window>
